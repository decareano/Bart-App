<h1>StaticPages#contact</h1>
<p>Find me in app/views/static_pages/contact.html.erb</p>


<br>

<% @user.errors.full_messages.each do |error| %>
<font color="red"><%= error %></font>
<% end %>
<br>


<%= form_for(@user) do |f| %>
<%= f.text_field :name %>
<%= f.label :name %>
<br>
<%= f.text_field :phone_number%>
<%= f.label :phone_number%>
<br>
<%=f.submit 'Make Me'%>
<% end %>
