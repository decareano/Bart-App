<h1>All Stations </h1>

<h2><%= link_to "Times for All Stations", all_stations_url %></h2>
<div id="stationsMap" class="centered">
	<ul id= "stationsList">
	<% @stations.each do |station| %>
	<li id= "station_<%= station.abbr %>" >
		<%= link_to station.name, station_url(station), id: station.id, class: "stationLink" %>
	</li>
	<% end %>
	</ul>
</div>

